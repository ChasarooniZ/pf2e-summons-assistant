{
  "id": "pf2e-summons-assistant",
  "title": "PF2e Summons Assistant",
  "description": "A module that supports summoning in PF2e out of the box, using Foundry Summons",
  "version": "#{VERSION}#",
  "library": "false",
  "compatibility": {
    "minimum": 12,
    "verified": 13
  },
  "authors": [
    {
      "name": "chasarooniZ",
      "url": "https://chasarooniz.github.io/"
    }
  ],
  "relationships": {
    "systems": [
      {
        "id": "pf2e",
        "type": "system"
      }
    ],
    "requires": [
      {
        "id": "foundry-summons",
        "type": "module",
        "manifest": "https://github.com/MrVauxs/foundry-summons-2/releases/latest/download/module.json",
        "compatibility": {
          "minimum": "2.2.0",
          "verified": "2.2.0"
        }
      }
    ],
    "recommends": [
      {
        "id": "pf2e-sustain-reminder",
        "name": "PF2e Sustain Reminder",
        "type": "module",
        "reason": "This module makes it easier to track what summons you are sustaining",
        "manifest": "https://github.com/Lank891/Foundry-Pf2e-Sustain-Reminder/releases/latest/download/module.json",
        "compatibility": {}
      }
    ]
  },
  "esmodules": ["scripts/module.js"],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "languages/en.json"
    }
  ],
  "url": "#{URL}#",
  "manifest": "#{MANIFEST}#",
  "download": "#{DOWNLOAD}#",
  "license": "LICENSE",
  "readme": "README.md"
}
