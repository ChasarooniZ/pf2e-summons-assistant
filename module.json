{
  "id": "pf2e-summons-assistant",
  "title": "PF2e Summons Assistant",
  "description": "A module that supports summoning in PF2e out of the box, using Foundry Summons",
  "version": "#{VERSION}#",
  "library": "false",
  "compatibility": {
    "minimum": 12,
    "verified": 13
  },
  "authors": [
    {
      "name": "chasarooniZ"
    }
  ],
  "relationships": {
    "systems": [
      {
        "id": "pf2e",
        "type": "system"
      }
    ],
    "requires": [
      {
        "id": "foundry-summons",
        "type": "module",
        "manifest": "https://github.com/MrVauxs/foundry-summons-2/releases/latest/download/module.json",
        "compatibility": {
          "minimum": "2.2.0",
          "verified": "2.2.0"
        }
      }
    ]
  },
  "esmodules": ["scripts/module.js"],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "languages/en.json"
    }
  ],
  "url": "#{URL}#",
  "manifest": "#{MANIFEST}#",
  "download": "#{DOWNLOAD}#",
  "license": "LICENSE",
  "readme": "README.md"
}
